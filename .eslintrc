{
  "extends": "eslint:recommended",
  "plugins": ["prettier"],
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": false,
    "browser": true
  },
  "rules": {
    "prettier/prettier": "error",
    "no-console": "off",
    "linebreak-style": "off"
  }
}
