  "scripts": {
    "build": "webpack --env=prod --progress --profile --colors --optimize-minimize --config ./config/webpack.prod.config.js",
    "start": "yarn run open-dev && webpack-dev-server --progress --inline --hot --config ./config/webpack.dev.config.js",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "open-dev": "opn http://localhost:8080/app/dev.html -- chrome",
    "open-prod": "opn http://localhost:8080/app/dev.html -- chrome"
  },
  "dependencies": {
    "@kadira/storybook": "^2.35.3",
    "glamor": "^2.20.25",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "opn-cli": "^3.1.0",
    "react-hot-loader": "^1.3.1"
